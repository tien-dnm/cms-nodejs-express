{
  "env": {
    "commonjs": false,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "quotes": "off",
    "semi": "off",
    "linebreak-style": "off",
    "comma-dangle": "off",
    "no-console": "off",
    "no-unused-vars": "warn",
    "consistent-return": "off",
    "no-trailing-spaces": "off",
    "camelcase": "off",
    "object-curly-newline": "off",
    "import/extensions": "off"
  }
}
